<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ImportLog">
    
	<select id="selectImportLog" parameterType="String" resultType="com.dahami.newsbank.web.dto.ImportLogDTO">
	    SELECT
	    	*
	    FROM
	    	importLog
	    WHERE
	    	uciCode=#{uciCode}
	    	AND
	    		uciCode IS NOT NULL
	    	AND
	    		(srcXml IS NOT NULL AND srcImg IS NOT NULL)
	    	AND
	    		result = 1
	    LIMIT 1
    </select>
	
</mapper>